@model MVCMARKMANAGE.Models.cfmap

@{
    ViewBag.Title = "cfmapview";
    Layout = "~/Views/User/userlayout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>cfmapview</title>
</head>
<body style="background-color:darkcyan">
    <center>
        <h1></h1>
        <br />


        @using (Html.BeginForm("cfmapview", "User", FormMethod.Post))
        {
            <table width="100%" border="1">
                <tr>
                    <td>Class Name : </td>
                    <td>
                        @Html.TextBoxFor(e => e.ClassID)
                    </td>
                </tr>
                <tr>
                    <td> Faculty Name : </td>
                    <td>
                        @Html.TextBoxFor(e => e.Fid)
                    </td>
                </tr>


                <tr>
                    <td colspan="2" align="center">
                        <input type="submit" value="Click Me" style="background-color:bisque" />
                    </td>
                </tr>
                <tr>
                    <td colspan="2">@ViewBag.update</td>
                </tr>
            </table>}
    </center>
</body>
</html>




